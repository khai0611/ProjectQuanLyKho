<Window x:Class="QuanLyKho.ReportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuanLyKho"
        mc:Ignorable="d"
        DataContext="{StaticResource ReportVM}"
        Title="ReportWindow" Height="450" Width="800">
    <Grid>
        <DocumentViewer 
            Name="DocViewer" 
            Document="{Binding FixedDocumentSequence, UpdateSourceTrigger=PropertyChanged}" 
            HorizontalAlignment="Left" Margin="0,10,-0.4,0" 
            VerticalAlignment="Top" 
            Height="410" Width="794" 
            Background="#FFF6E9E9"/>
    </Grid>
</Window>
